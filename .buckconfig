[cxx]
  default_platform = iphonesimulator-x86_64
  combined_preprocess_and_compile = true

[apple]
  iphonesimulator_target_sdk_version = 5.0
  iphoneos_target_sdk_version = 5.0
  macosx_target_sdk_version = 10.7

[alias]
  library = //:PINCache

[project]
  ide = xcode
  ignore = .buckd, \
           .hg, \
           .git, \
           buck-out, \
